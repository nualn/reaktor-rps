  "bugs": {
    "url": "https://github.com/nualn/reaktor-rps/issues"
  },
  "homepage": "https://github.com/nualn/reaktor-rps#readme",
  "workspaces": [
    "packages/client",
    "packages/server"
  ],
  "dependencies": {
    "dotenv": "^14.1.0"
  }
}
